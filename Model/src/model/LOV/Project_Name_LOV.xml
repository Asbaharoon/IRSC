<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Project_Name_LOV"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  SelectList="TblProject.ID, 
       TblProject.NAME, 
       TblProject.AST, 
       TblProject.COMPANY_ID"
  FromList="TBL_PROJECT TblProject"
  Where="TblProject.AST='Y'  
And TblProject.COMPANY_ID = nvl(:P_COMPANY_ID,TblProject.COMPANY_ID)"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.Project_Name_LOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P_COMPANY_ID"
    Kind="where"
    Type="java.math.BigDecimal">
    <TransientExpression
      Name="VariableScript"
      CodeSourceName="Project_Name_LOV"/>
  </Variable>
  <EntityUsage
    Name="TblProject"
    Entity="model.ERP.EO.TblProject"/>
  <ViewAttribute
    Name="Id"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="TblProject"
    AliasName="ID"/>
  <ViewAttribute
    Name="Name"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Name"
    EntityUsage="TblProject"
    AliasName="NAME"/>
  <ViewAttribute
    Name="Ast"
    PrecisionRule="true"
    EntityAttrName="Ast"
    EntityUsage="TblProject"
    AliasName="AST"/>
  <ViewAttribute
    Name="CompanyId"
    PrecisionRule="true"
    EntityAttrName="CompanyId"
    EntityUsage="TblProject"
    AliasName="COMPANY_ID"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
