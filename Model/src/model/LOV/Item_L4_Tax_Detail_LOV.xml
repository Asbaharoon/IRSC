      CodeSourceName="Item_L4_Tax_Detail_LOV"/>
  </Variable>
  <EntityUsage
    Name="TblItemL4"
    Entity="model.ERP.EO.TblItemL4"/>
  <EntityUsage
    Name="TblTaxDetail"
    Entity="model.ERP.EO.TblTaxDetail"
    ReadOnly="true"
    DeleteParticipant="false">
    <DesignTime>
      <Attr Name="_customjoin" Value="true"/>
    </DesignTime>
  </EntityUsage>
  <ViewAttribute
    Name="ItemL4Id"
    PrecisionRule="true"
    EntityAttrName="ItemL4Id"
    EntityUsage="TblItemL4"
    AliasName="ITEM_L4_ID"/>
  <ViewAttribute
    Name="Name"
    IsUnique="true"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Name"
    EntityUsage="TblItemL4"
    AliasName="NAME"/>
  <ViewAttribute
    Name="Ast"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Ast"
    EntityUsage="TblItemL4"
    AliasName="AST"/>
