    Name="CompanyId"
    PrecisionRule="true"
    EntityAttrName="CompanyId"
    EntityUsage="TblItemL4"
    AliasName="COMPANY_ID"/>
  <ViewAttribute
    Name="TaxDetailId"
    PrecisionRule="true"
    EntityAttrName="TaxDetailId"
    EntityUsage="TblItemL4"
    AliasName="TAX_DETAIL_ID"/>
  <ViewAttribute
    Name="TaxPerc"
    PrecisionRule="true"
    EntityAttrName="TaxPerc"
    EntityUsage="TblTaxDetail"
    AliasName="TAX_PERC"/>
  <ViewAttribute
    Name="Id1"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="TblTaxDetail"
    AliasName="ID"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
