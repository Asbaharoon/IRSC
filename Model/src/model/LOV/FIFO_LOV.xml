<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="FIFO_LOV"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.FIFO_LOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="get_Proj_ID"
    Kind="where"
    Type="java.math.BigDecimal"/>
  <Variable
    Name="get_dept_ID"
    Kind="where"
    Type="java.math.BigDecimal"/>
  <SQLQuery><![CDATA[select             
VF.ID, VF.Item_L4_ID, VF.Item_Name, VF.bal_qty ,VF.bal_amt/VF.bal_qty rate,   
RD.RATE RD_RATE   
            
from VEW_FIFO VF,TBL_RDF_MASTER RM,TBL_RDF_DETAIL RD   
   
where   
VF.ITEM_L4_ID = RD.ITEM_L4_ID   
and RM.ID = RD.RDF_MASTER_ID   
and VF.project_id = nvl(:get_Proj_ID,VF.project_id)        
and VF.Department_id = nvl(:get_dept_ID,VF.Department_id)]]></SQLQuery>
  <ViewAttribute
    Name="Id"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="ID"
    Expression="ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="ItemL4Id"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="ITEM_L4_ID"
    Expression="ITEM_L4_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="ItemName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="150"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITEM_NAME"
    Expression="ITEM_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="150"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="BalQty"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="BAL_QTY"
    Expression="BAL_QTY"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Rate"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="RATE"
    Expression="RATE"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="RDRATE"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="RD_RATE"
    SQLType="NUMERIC"
    Expression="RD_RATE"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="Id"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
