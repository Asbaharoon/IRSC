<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Unit_of_Measurements_LOV"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  SelectList="TblUom.ID, 
       TblUom.NAME, 
       TblUom.AST, 
       TblUom.COMPANY_ID"
  FromList="TBL_UOM TblUom"
  Where="TblUom.AST='Y'   
AND TblUom.COMPANY_ID=nvl(:P_COMPANY_ID,TblUom.COMPANY_ID)"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.Unit_of_Measurements_LOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P_COMPANY_ID"
    Kind="where"
    Type="java.math.BigDecimal">
    <TransientExpression
      Name="VariableScript"
      trustMode="trusted"
      CodeSourceName="Unit_of_Measurements_LOV"/>
  </Variable>
  <EntityUsage
    Name="TblUom"
    Entity="model.ERP.EO.TblUom"/>
  <ViewAttribute
    Name="Id"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="TblUom"
    AliasName="ID"/>
  <ViewAttribute
    Name="Name"
    PrecisionRule="true"
    EntityAttrName="Name"
    EntityUsage="TblUom"
    AliasName="NAME"/>
  <ViewAttribute
    Name="Ast"
    PrecisionRule="true"
    EntityAttrName="Ast"
    EntityUsage="TblUom"
    AliasName="AST"/>
  <ViewAttribute
    Name="CompanyId"
    PrecisionRule="true"
    EntityAttrName="CompanyId"
    EntityUsage="TblUom"
    AliasName="COMPANY_ID"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
