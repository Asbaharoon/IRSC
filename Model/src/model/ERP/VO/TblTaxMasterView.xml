<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="TblTaxMasterView"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  SelectList="TblTaxMaster.ID, 
       TblTaxMaster.NAME, 
       TblTaxMaster.AST, 
       TblTaxMaster.CREATED_DATE, 
       TblTaxMaster.CREATED_BY, 
       TblTaxMaster.UPDATED_DATE, 
       TblTaxMaster.UPDATED_BY, 
       TblTaxMaster.COMPANY_ID"
  FromList="TBL_TAX_MASTER TblTaxMaster"
  Where="TblTaxMaster.COMPANY_ID=Nvl(:P_COMPANY_ID,TblTaxMaster.COMPANY_ID)">
  <Variable
    Name="P_COMPANY_ID"
    Kind="where"
    Type="java.math.BigDecimal">
    <TransientExpression
      Name="VariableScript"
      trustMode="trusted"
      CodeSourceName="TblTaxMasterView"/>
  </Variable>
  <ViewAccessor
    Name="Active_LOV1"
    ViewObjectName="model.LOV.Active_LOV"
    RowLevelBinds="true"/>
  <ListBinding
    Name="LOV_Ast"
    ListVOName="Active_LOV1"
    ListRangeSize="-1"
    NullValueFlag="none"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="Ast"/>
    </AttrArray>
    <AttrArray Name="AttrExpressions"/>
    <AttrArray Name="ListAttrNames">
      <Item Value="DBAttr"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="ViewAttr"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <EntityUsage
    Name="TblTaxMaster"
    Entity="model.ERP.EO.TblTaxMaster"/>
  <ViewAttribute
    Name="Id"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="TblTaxMaster"
    AliasName="ID"/>
  <ViewAttribute
    Name="Name"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Name"
    EntityUsage="TblTaxMaster"
    AliasName="NAME"/>
  <ViewAttribute
    Name="Ast"
    PrecisionRule="true"
    EntityAttrName="Ast"
    EntityUsage="TblTaxMaster"
    AliasName="AST"
    LOVName="LOV_Ast">
    <Properties>
      <SchemaBasedProperties>
        <CONTROLTYPE
          Value="combo"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="CreatedDate"
    PrecisionRule="true"
    EntityAttrName="CreatedDate"
    EntityUsage="TblTaxMaster"
    AliasName="CREATED_DATE"/>
  <ViewAttribute
    Name="CreatedBy"
    PrecisionRule="true"
    EntityAttrName="CreatedBy"
    EntityUsage="TblTaxMaster"
    AliasName="CREATED_BY"/>
  <ViewAttribute
    Name="UpdatedDate"
    PrecisionRule="true"
    EntityAttrName="UpdatedDate"
    EntityUsage="TblTaxMaster"
    AliasName="UPDATED_DATE"/>
  <ViewAttribute
    Name="UpdatedBy"
    PrecisionRule="true"
    EntityAttrName="UpdatedBy"
    EntityUsage="TblTaxMaster"
    AliasName="UPDATED_BY"/>
  <ViewAttribute
    Name="CompanyId"
    PrecisionRule="true"
    EntityAttrName="CompanyId"
    EntityUsage="TblTaxMaster"
    AliasName="COMPANY_ID"/>
  <ViewLinkAccessor
    Name="TblTaxDetailView"
    ViewLink="model.ERP.VO.FkTaxMasterLink"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
</ViewObject>
