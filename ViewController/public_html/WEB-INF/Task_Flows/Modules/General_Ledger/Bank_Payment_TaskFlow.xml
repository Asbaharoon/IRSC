<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="Bank_Payment_TaskFlow">
    <default-activity>Execute</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__5">
      <managed-bean-name>FileManagingBean</managed-bean-name>
      <managed-bean-class>view.Image.FileManaging</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__6">
      <managed-bean-name>UploadBeanBPV</managed-bean-name>
      <managed-bean-class>view.Image.UploadBeanBPV</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__7">
      <managed-bean-name>UploadBeanBPVImg</managed-bean-name>
      <managed-bean-class>view.Image.UploadBeanBPVImg</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__8">
      <managed-bean-name>BankCashOpenFragmentBean</managed-bean-name>
      <managed-bean-class>view.Pages.BankCashOpenFragment</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <view id="Bank_Payment">
      <page>/WEB-INF/Fragments/Modules/General_Ledger/Bank_Payment.jsff</page>
    </view>
    <method-call id="ExecuteWithParams">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ExecuteWithParams</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="Execute">
      <method>#{bindings.Execute.execute}</method>
      <outcome>
        <fixed-outcome>Execute</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>ExecuteWithParams</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>ExecuteWithParams</from-outcome>
        <to-activity-id>Bank_Payment</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>Execute</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>Execute</from-outcome>
        <to-activity-id>ExecuteWithParams</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
