    <default-activity>VAT_Return</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>VATReturnReportBackBean</managed-bean-name>
      <managed-bean-class>view.Report.VATReturnReport</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="VAT_Return">
      <page>/WEB-INF/Fragments/Modules/Reports/VAT_Return.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
