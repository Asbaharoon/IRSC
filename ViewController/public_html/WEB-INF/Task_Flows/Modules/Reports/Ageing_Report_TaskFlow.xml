<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="Ageing_Report_TaskFlow">
    <default-activity>Ageing_Report</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>AgeingReportBackBean</managed-bean-name>
      <managed-bean-class>view.Report.AgeingReport</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="Ageing_Report">
      <page>/WEB-INF/Fragments/Modules/Reports/Ageing_Report.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
